  <LuaScript>local processname = "gta3.exe"

function SetStateActiveCheats(state, onlyActiveScript)
    local addressList = getAddressList()
    local count = addressList.Count

    for i=0,count-1 do
        local typeMemoryRectod = addressList[i].Type
        if typeMemoryRectod == vtAutoAssembler then
          if onlyActiveScript then
